<div>
    <%- partial('_partial/article', {index: false, post: page}) %>
</div>

<div id="post-footer">
    <div id="page_next">
        <a href="<%- config.root %><%- page.next.path %>" title="<%= page.next.title %>">
            上一篇：<%= page.next.title %>
        </a>
    </div>
    
    
    <div id="page_prev">
        <a href="<%- config.root %><%- page.prev.path %>" title="<%= page.prev.title %>">
            下一篇：<%= page.prev.title %>
        </a>
    </div>
</div>
